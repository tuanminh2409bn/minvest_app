{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "minvestforexapp-33dff",
          "appId": "1:245218403052:android:8da5ef21fd9e851bb03bc9",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "minvestforexapp-33dff",
          "configurations": {
            "android": "1:245218403052:android:8da5ef21fd9e851bb03bc9",
            "ios": "1:245218403052:ios:c5d314db21e0ee24b03bc9",
            "web": "1:245218403052:web:30b6a6e919f731eeb03bc9"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  }
}